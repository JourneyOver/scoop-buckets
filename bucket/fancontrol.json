{
    "version": "145",
    "description": "A highly customizable fan controlling software for the Windows OS.",
    "homepage": "https://getfancontrol.com/",
    "license": "Freeware",
    "url": "https://raw.githubusercontent.com/Rem0o/FanControl.Releases/master/FanControl.zip",
    "hash": "b9c4b5ee56999405bcbf4c7cdd80bdf7df635cdeebec8fd78bae0422b23c63ae",
    "pre_install": [
        "'CACHE', 'Plugins' | ForEach-Object {",
        "    if (!(Test-Path \"$persist_dir\\$_\")) { New-Item \"$dir\\$_\" -ItemType Directory | Out-Null }",
        "}",
        "if (!(Test-Path \"$persist_dir\\userConfig.json\")) {",
        "    New-Item \"$dir\\userConfig.json\" | Out-Null",
        "}"
    ],
    "shortcuts": [
        [
            "FanControl.exe",
            "FanControl"
        ]
    ],
    "persist": [
        "userConfig.json",
        "CACHE",
        "Plugins"
    ],
    "checkver": {
        "url": "https://raw.githubusercontent.com/Rem0o/FanControl.Releases/master/version.json",
        "jsonpath": "$.Number"
    },
    "autoupdate": {
        "url": "https://raw.githubusercontent.com/Rem0o/FanControl.Releases/master/FanControl.zip"
    }
}
